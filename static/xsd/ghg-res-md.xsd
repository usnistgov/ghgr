  <xs:complexType name="Measures">
    <xs:annotation>
      <xs:documentation>
           a container for metrology-specific metadata
         </xs:documentation>
    </xs:annotation>

    <xs:sequence>

      <xs:element name="propertyName" type="xs:string" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:appinfo>
                 <label>Name of property measured: </label>
                 <tooltip>Click the green plus to add more property names</tooltip>
               </xs:appinfo>
          <xs:documentation>
                  the name of a property measured or calculated
               </xs:documentation>
          <xs:documentation>
                  This should be drawn, when possible, from ISO 80000.  
               </xs:documentation>
        </xs:annotation>
      </xs:element>


      <xs:element name="qualityPractices" type="rsm:QualityMetric" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
                 <label>Quality Practices: Please answer the following questions: </label>
               </xs:appinfo>
          <xs:documentation>
                 a metrology practice employed to help ensure that the resulting
                 data is of good quality.  
               </xs:documentation>
        </xs:annotation>
      </xs:element>


    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="QualityMetric">
    <xs:annotation>
      <xs:documentation>
         labels for specific quality metrics
       </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="calibratedEquipment" type="rsm:YesNo" minOccurs="1" maxOccurs="1"
        default="No (or Don't Know)">
        <xs:annotation>
          <xs:appinfo>
              <label>Did the measurements make use of equpment set with standard calibrations, traceable to recognized primary standards and SI units? </label>
            </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="standardMethods" type="rsm:YesNo" minOccurs="1" maxOccurs="1"
        default="No (or Don't Know)">
        <xs:annotation>
          <xs:appinfo>
              <label>Was a recognized standard method used to obtain the measurements? </label>
            </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="characterizedUncertainties" type="rsm:YesNo" minOccurs="1" maxOccurs="1"
        default="No (or Don't Know)">
        <xs:annotation>
          <xs:appinfo>
              <label>Do the reported values include a full GUM-based characterization of uncertainties? </label>
            </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="multisiteMeasurements" type="rsm:YesNo" minOccurs="1" maxOccurs="1"
        default="No (or Don't Know)">
        <xs:annotation>
          <xs:appinfo>
              <label>Are the reported values based on multi-site (e.g. round-robin) measurements? </label>
            </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="sampleStability" type="rsm:YesNo" minOccurs="1" maxOccurs="1"
        default="No (or Don't Know)">
        <xs:annotation>
          <xs:appinfo>
              <label>Are the reported values accompanied by a description of the stability and/or maturity of the the samples measured? </label>
            </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="referenceMaterials" type="rsm:YesNo" minOccurs="1" maxOccurs="1"
        default="No (or Don't Know)">
        <xs:annotation>
          <xs:appinfo>
              <label>Were the measurements conducted on certified reference materials (as defined by VIM3)? </label>
            </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="reviewAndCertification" type="rsm:YesNo" minOccurs="1" maxOccurs="1"
        default="No (or Don't Know)">
        <xs:annotation>
          <xs:appinfo>
              <label>Are the reported values accompanied by documentation of certification, a review report, or a reference to a peer-reviewed open-access paper describing the quality of the data? </label>
            </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="criticalLiteratureReview" type="rsm:YesNo" minOccurs="1" maxOccurs="1"
        default="No (or Don't Know)">
        <xs:annotation>
          <xs:appinfo>
              <label>Are the reported values compiled through a critical review of values reported in the peer-reviewed literature? </label>
            </xs:appinfo>
        </xs:annotation>
      </xs:element>
    </xs:sequence>

  </xs:complexType>

  <xs:simpleType name="YesNo">
    <xs:annotation>
      <xs:documentation>
